#include <stdio.h>
int main(void) {
	int i,t;
	scanf("%d",&t);
	for(i=0;i<t;i++)
	{
	    long long int n;
	    scanf("%lld",&n);
	    long long int a[n],sum,j;
	    sum=0;
	    for(j=0;j<n;j++)
	    {
	        scanf("%lld",&a[j]);
	        sum+=a[j];
	    }
	    for(j=0;j<n;j++)
	    {
	        printf("%lld ",sum-a[j]);
	    }
	    printf("\n");
	}
	return 0;
}


